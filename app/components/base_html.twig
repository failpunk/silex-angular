<!DOCTYPE html>
<html lang="en" ng-app="app">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title></title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->

      {% block css %}
        <link rel="stylesheet" href="js/vendor/bootstrap/dist/css/bootstrap.min.css">
        <link rel="stylesheet" href="assetic/css/main.css">
      {% endblock %}
    </head>
    <body ng-cloak>

    <div class="site-wrapper">

      <div class="site-wrapper-inner">

        <div class="cover-container">

          <div ng-include="'components/navBar/templates/navBar.html'"></div>


          <!-- Add your site or application content here -->
          <div class="inner cover">
          {% block content %}
            <h1 class="cover-heading">Default Content.</h1>
          {% endblock %}
          </div>


          <div class="mastfoot">
            <div class="inner">
              {% block footer %}
                <p>silex-angular on <a href="https://github.com/failpunk/silex-angular">github</a>, by <a href="https://twitter.com/failpunk">@failpunk</a>.</p>
              {% endblock %}
            </div>
          </div>

        </div>

      </div>

    </div>


      {% block javascript %}
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
        <script>window.jQuery || document.write('<script src="js/vendor/jquery/dist/jquery.min.js"><\/script>')</script>
        <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.12/angular.min.js"></script>
        <script src="assetic/js/vendor.js"></script>
        <script src="assetic/js/main.js"></script>
      {% endblock %}

    </body>
</html>